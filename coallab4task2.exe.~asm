.model small
.stack 100h
.data
msg db "Maria - SAP ID: 68647$"

.code
main proc
mov ax, @data
mov ds, ax

mov dx, offset msg
mov ah, 09h        ; Display string
int 21h

mov ah, 4ch        ; Exit program
int 21h
main endp
end main

